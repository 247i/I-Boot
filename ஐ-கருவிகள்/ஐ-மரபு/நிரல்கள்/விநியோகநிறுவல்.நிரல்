	;  !insertmacro கோப்பில்மாற்று "CONFIG boot" "CONFIG /!/$JustISOName/arch/boot" "all" "all" "$BootDir\!\$JustISOName\arch\boot\syslinux\archiso_sys_choose.cfg"  
	;  !insertmacro கோப்பில்மாற்று "LINUX boot" "LINUX /!/$JustISOName/arch/boot" "all" "all" "$BootDir\!\$JustISOName\arch\boot\syslinux\archiso_sys32.cfg"  
	;  !insertmacro கோப்பில்மாற்று "INITRD boot" "INITRD /!/$JustISOName/arch/boot" "all" "all" "$BootDir\!\$JustISOName\arch\boot\syslinux\archiso_sys32.cfg"
	  !insertmacro கோப்பில்மாற்று "archisolabel=justbrowsing" "archisolabel=TA" "all" "all" "$BootDir\!\$JustISOName\arch\boot\syslinux\archiso_sys32.cfg"   
	;  !insertmacro கோப்பில்மாற்று "LINUX boot" "LINUX /!/$JustISOName/arch/boot" "all" "all" "$BootDir\!\$JustISOName\arch\boot\syslinux\archiso_sys64.cfg"  
	;  !insertmacro கோப்பில்மாற்று "INITRD boot" "INITRD /!/$JustISOName/arch/boot" "all" "all" "$BootDir\!\$JustISOName\arch\boot\syslinux\archiso_sys64.cfg"
	  !insertmacro கோப்பில்மாற்று "archisolabel=justbrowsing" "archisolabel=TA" "all" "all" "$BootDir\!\$JustISOName\arch\boot\syslinux\archiso_sys64.cfg"  
	;  !insertmacro கோப்பில்மாற்று "COM32 boot" "COM32 /!/$JustISOName/arch/boot" "all" "all" "$BootDir\!\$JustISOName\arch\boot\syslinux\archiso_tail.cfg"  
	;  !insertmacro கோப்பில்மாற்று "LINUX boot" "LINUX /!/$JustISOName/arch/boot" "all" "all" "$BootDir\!\$JustISOName\arch\boot\syslinux\archiso_timezones.cfg"  
	;  !insertmacro கோப்பில்மாற்று "INITRD boot" "INITRD /!/$JustISOName/arch/boot" "all" "all" "$BootDir\!\$JustISOName\arch\boot\syslinux\archiso_timezones.cfg"
	  !insertmacro கோப்பில்மாற்று "archisolabel=justbrowsing" "archisolabel=TA" "all" "all" "$BootDir\!\$JustISOName\arch\boot\syslinux\archiso_timezones.cfg" 
	;  !insertmacro கோப்பில்மாற்று "LINUX boot" "LINUX /!/$JustISOName/arch/boot" "all" "all" "$BootDir\!\$JustISOName\arch\boot\syslinux\archiso_vbox.cfg"  
	;  !insertmacro கோப்பில்மாற்று "INITRD boot" "INITRD /!/$JustISOName/arch/boot" "all" "all" "$BootDir\!\$JustISOName\arch\boot\syslinux\archiso_vbox.cfg"
	  !insertmacro கோப்பில்மாற்று "archisolabel=justbrowsing" "archisolabel=TA" "all" "all" "$BootDir\!\$JustISOName\arch\boot\syslinux\archiso_vbox.cfg" 
	  !insertmacro கோப்பில்மாற்று "archisobasedir=arch" "archisobasedir=/!/$JustISOName/arch" "all" "all" "$BootDir\!\$JustISOName\arch\boot\syslinux\archiso_vbox.cfg"  
	;  !insertmacro கோப்பில்மாற்று "LINUX boot" "LINUX /!/$JustISOName/arch/boot" "all" "all" "$BootDir\!\$JustISOName\arch\boot\syslinux\archiso_videomodes.cfg"  
	;  !insertmacro கோப்பில்மாற்று "INITRD boot" "INITRD /!/$JustISOName/arch/boot" "all" "all" "$BootDir\!\$JustISOName\arch\boot\syslinux\archiso_videomodes.cfg"
	  !insertmacro கோப்பில்மாற்று "archisolabel=justbrowsing" "archisolabel=TA" "all" "all" "$BootDir\!\$JustISOName\arch\boot\syslinux\archiso_videomodes.cfg" 
	  !insertmacro கோப்பில்மாற்று "archisobasedir=arch" "archisobasedir=/!/$JustISOName/arch" "all" "all" "$BootDir\!\$JustISOName\arch\boot\syslinux\archiso_videomodes.cfg"    
	;  !insertmacro கோப்பில்மாற்று "LINUX boot" "LINUX /!/$JustISOName/arch/boot" "all" "all" "$BootDir\!\$JustISOName\arch\boot\syslinux\archiso_vmware.cfg"  
	;  !insertmacro கோப்பில்மாற்று "INITRD boot" "INITRD /!/$JustISOName/arch/boot" "all" "all" "$BootDir\!\$JustISOName\arch\boot\syslinux\archiso_vmware.cfg"
	  !insertmacro கோப்பில்மாற்று "archisolabel=justbrowsing" "archisolabel=TA" "all" "all" "$BootDir\!\$JustISOName\arch\boot\syslinux\archiso_vmware.cfg"
	  !insertmacro கோப்பில்மாற்று "archisobasedir=arch" "archisobasedir=/!/$JustISOName/arch" "all" "all" "$BootDir\!\$JustISOName\arch\boot\syslinux\archiso_vmware.cfg"    
	 
	; Xiaopan 
	  ${ElseIf} $DistroName == "Xiaopan (Penetration Testing)"   
	  ;CopyFiles "$BootDir\!\$JustISOName\cde\*.*" "$BootDir\cde\" ;(quick hack until a cde bootcode/cheatcode is made upstream from tinyCore)
	  ;CopyFiles "$BootDir\!\$JustISOName\mydata.tgz" "$BootDir\mydata.tgz"
	  !insertmacro கோப்பில்மாற்று "MENU BACKGROUND /boot/isolinux/splash.jpg" "MENU BACKGROUND /!/$JustISOName/boot/isolinux/splash.jpg" "all" "all" "$BootDir\!\$JustISOName\$CopyPath\$ConfigFile"
	  !insertmacro கோப்பில்மாற்று "MENU BACKGROUND /boot/isolinux/splash.jpg" "MENU BACKGROUND /!/$JustISOName/boot/isolinux/splash.jpg" "all" "all" "$BootDir\!\$JustISOName\$CopyPath\$ConfigFile"
	  
	; Ophcrack
	 ${ElseIf} ${FileExists} "$BootDir\!\$JustISOName\$CopyPath\ophcrack.cfg"  
	  CopyFiles "$BootDir\!\$JustISOName\tables\*.*" "$BootDir\tables\"
	  RMDir /R "$BootDir\!\$JustISOName\tables"  
	 ${EndIf} 
	 
	; OpenSuse/BlehOS  
	   ${If} ${FileExists} "$BootDir\!\$JustISOName\boot\i386\loader\isolinux.cfg"       
	   !insertmacro கோப்பில்மாற்று "ui gfxboot" "#ui NULL gfxboot" "all" "all" "$BootDir\!\$JustISOName\boot\i386\loader\isolinux.cfg"     
	  ${EndIf}   
	  
	; For OpenSuSe like compilations!
	 ${If} ${FileExists} "$BootDir\!\$JustISOName\boot\grub\mbrid" 
	  StrCpy $ConfigFile == "syslinux.cfg" ; Make sure it isn't NULL  
	  StrCpy $SUSEDIR "$JustISOName" 
	  Call MBRID  
	 ${EndIf}   

	; Enable Casper  
		 ${If} $Persistence == "casper" ; Casper
			  ${AndIf} $Casper != "0" ; Casper Slider (Size) was not Null
			  ; Add Boot Code Persistent
			  ${If} ${FileExists} "$BootDir\!\$JustISOName\isolinux\txt.cfg" ; Rename the following for isolinux txt.cfg
			  !insertmacro கோப்பில்மாற்று "cdrom-detect/try-usb=true noprompt" "cdrom-detect/try-usb=true persistent persistent-path=/!/$JustISOName noprompt" "all" "all" "$BootDir\!\$JustISOName\isolinux\txt.cfg" 
		 ${EndIf}  
	  
		 ${If} ${FileExists} "$BootDir\!\$JustISOName\isolinux\text.cfg" ; Rename the following for isolinux text.cfg
			 !insertmacro கோப்பில்மாற்று "cdrom-detect/try-usb=true noprompt" "cdrom-detect/try-usb=true persistent persistent-path=/!/$JustISOName noprompt" "all" "all" "$BootDir\!\$JustISOName\isolinux\text.cfg" 
		 ${EndIf}
	  
		 ${If} ${FileExists} "$BootDir\!\$JustISOName\isolinux\isolinux.cfg" ; Rename the following for isolinux.cfg
			 !insertmacro கோப்பில்மாற்று "cdrom-detect/try-usb=true noprompt" "cdrom-detect/try-usb=true persistent persistent-path=/!/$JustISOName noprompt" "all" "all" "$BootDir\!\$JustISOName\isolinux\isolinux.cfg"    
		 ${EndIf}
		
		 ${If} ${FileExists} "$BootDir\!\$JustISOName\isolinux\menuentries.cfg" ; Rename the following for menuentries.cfg
			 !insertmacro கோப்பில்மாற்று "cdrom-detect/try-usb=true noprompt" "cdrom-detect/try-usb=true persistent persistent-path=/!/$JustISOName noprompt" "all" "all" "$BootDir\!\$JustISOName\isolinux\menuentries.cfg"    
		 ${EndIf}
		
		 ${If} ${FileExists} "$BootDir\!\$JustISOName\boot\grub\loopback.cfg" ; Rename the following for grub loopback.cfg
			 !insertmacro கோப்பில்மாற்று "cdrom-detect/try-usb=true noprompt" "cdrom-detect/try-usb=true persistent persistent-path=/!/$JustISOName noprompt" "all" "all" "$BootDir\!\$JustISOName\boot\grub\loopback.cfg"  
		 ${EndIf} 
		 ; Create Casper-rw file
		 Call CasperScript  
		 Call WriteStuff
    ${EndIf}
!macroend