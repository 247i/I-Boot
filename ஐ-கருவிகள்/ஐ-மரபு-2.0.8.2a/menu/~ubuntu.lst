# This Menu created with YUMI https://www.pendrivelinux.com/yumi-!-usb-creator/
default 0
timeout 30
color NORMAL HIGHLIGHT HELPTEXT HEADING
foreground=FFFFFF
background=000000

title Boot DLUG
set ISO=/!/DLUG/SLUG
set CASPER=/!/DLUG/writable
find --set-root %ISO%
map %ISO% (0xff)
#CLUG
map --hook
root (0xff)
kernel /casper/VLUG file=/cdrom/preseed/SLEED noprompt boot=casper cdrom-detect/try-usb=true iso-scan/filename=%ISO% --
initrd /casper/ILUG

title Boot DLUG acpi=off non-persistent
set ISO=/!/DLUG/SLUG
set CASPER=/!/DLUG/writable
find --set-root %ISO%
map %ISO% (0xff)
#CLUG
map --hook
root (0xff)
kernel /casper/VLUG file=/cdrom/preseed/SLEED boot=casper iso-scan/filename=%ISO% acpi=off --
initrd /casper/ILUG